{
  "name": "blog-embedded-metrics-3",
  "version": "1.0.0",
  "description": "Source code for blog post on embedded metrics.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint-and-fix": "eslint ./src --ext .ts --fix",
    "build": "npm run lint && tsc",
    "test": "mocha --require ts-node/register test/**/*.ts",
    "test-coverage": "nyc npm run test",
    "mock-api-cdk-synth": "tsc && cdk synth MockApiStack > .cdk-synth.yml",
    "mock-api-cdk-deploy": "tsc && cdk deploy MockApiStack",
    "mock-api-cdk-hotswap": "tsc && cdk deploy MockApiStack --hotswap",
    "loan-processor-cdk-synth": "tsc && cdk synth LoanProcessorTestStack > .cdk-synth.yml",
    "loan-processor-cdk-deploy": "tsc && cdk deploy LoanProcessorTestStack",
    "loan-processor-cdk-hotswap": "tsc && cdk deploy LoanProcessorTestStack --hotswap",
    "cdk-deploy-all": "tsc && cdk deploy MockApiStack && cdk deploy LoanProcessorTestStack",
    "cdk-destroy-all": "tsc && cdk destroy --all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andybalham/blog-embedded-metrics-3.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andybalham/blog-embedded-metrics-3/issues"
  },
  "homepage": "https://github.com/andybalham/blog-embedded-metrics-3#readme",
  "devDependencies": {
    "@andybalham/sls-testing-toolkit": "^1.0.12",
    "@aws-cdk/aws-apigateway": "^1.129.0",
    "@aws-cdk/aws-dynamodb": "^1.129.0",
    "@aws-cdk/aws-events-targets": "^1.129.0",
    "@aws-cdk/aws-lambda": "^1.129.0",
    "@aws-cdk/aws-lambda-event-sources": "^1.129.0",
    "@aws-cdk/aws-lambda-nodejs": "^1.129.0",
    "@aws-cdk/aws-sns": "^1.129.0",
    "@aws-cdk/aws-sns-subscriptions": "^1.129.0",
    "@aws-cdk/aws-sqs": "^1.129.0",
    "@aws-cdk/aws-stepfunctions": "^1.129.0",
    "@aws-cdk/aws-stepfunctions-tasks": "^1.129.0",
    "@aws-cdk/aws-cloudwatch": "^1.129.0",
    "@aws-cdk/aws-cloudwatch-actions": "^1.129.0",
    "@aws-cdk/core": "^1.129.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/aws-lambda": "^8.10.81",
    "@types/chai": "^4.2.21",
    "@types/luxon": "^2.0.5",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "aws-sdk": "^2.958.0",
    "chai": "^4.3.4",
    "esbuild": "^0.12.17",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@andybalham/state-machine-builder": "^1.0.12",
    "@andybalham/state-machine-with-graph": "^1.0.6",
    "@aws-cdk/aws-apigatewayv2": "^1.129.0",
    "@aws-cdk/aws-apigatewayv2-integrations": "^1.129.0",
    "aws-embedded-metrics": "^2.0.4",
    "axios": "^0.25.0",
    "nanoid": "^3.2.0"
  }
}
